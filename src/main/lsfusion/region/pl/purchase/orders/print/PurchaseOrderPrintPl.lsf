MODULE PurchaseOrderPrintPl;

REQUIRE PurchaseOrderPrint, LegalEntityPl, UtilsPl;

NAMESPACE Invoicing;

// Polish specific identifiers for company and customer
nipCompany (Order i) = nip(company(i));
nipVendor (Order i) = nip(vendor(i));

// Extend base print form with PL-specific properties
EXTEND FORM printOrder
    PROPERTIES(o) nipCompany, nipVendor
;
