MODULE ReceiptInitial;

REQUIRE ReceiptType, ReceiptCost;

NAMESPACE Inventory;

loadInitialData () + {
    NEW r = ReceiptType {
        name(r) <- 'Поступление';
        id(r) <- 'receipt';
        NEW n = Numerator {
            name(n) <- 'Поступление';
            series(n) <- 'ПП';
            minValue(n) <- 1;
            maxValue(n) <- 999999;
            stringLength(n) <- 6;
            numerator(r) <- n;
        }
    }
    NEW r = ReceiptType {
        name(r) <- 'Ввод начальных остатков';
        id(r) <- 'initial';
        showCost(r) <- TRUE;
        NEW n = Numerator {
            name(n) <- 'Ввод начальных остатков';
            series(n) <- 'НО';
            minValue(n) <- 1;
            maxValue(n) <- 999999;
            stringLength(n) <- 6;
            numerator(r) <- n;
        }
    }
}