MODULE MasterDataSettings;

REQUIRE MasterData;

NAMESPACE MasterData;

FORM options 'Settings';

DESIGN options {
    OBJECTS {
        NEW tabbedPane {
            tabbed = TRUE;
            fill = 1;
            NEW commons {
                caption = 'Main';
            }
        }
    }
}

NAVIGATOR {
    masterData {
        NEW options;
    }
}